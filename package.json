  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "express": "4.19.2",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "tsx": "4.17.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-solid": "^2.10.2"
  }
}
